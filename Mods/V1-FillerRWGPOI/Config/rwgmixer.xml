<configs>

	<!-- Gives Old West a Trader and Gateway potential -->
	<set xpath="/rwgmixer/township[@name='oldwest']/property[@name='spawn_trader']/@value">true</set>
	<set xpath="/rwgmixer/township[@name='oldwest']/property[@name='spawn_gateway']/@value">true</set>
	<append xpath="/rwgmixer/township[@name='oldwest']/property[@name='biomes']/@value">,forest,burntforest,snow</append><!-- Expanding desert -->
	
	<!-- Tiny 0 - 5000 RWG Fillers -->
	<set xpath="/rwgmixer/world[@name='tiny']/property[@class='oldwest']/property[@name='tiles']/@value">2, 3</set> <!-- 1, 1 -->
	<set xpath="/rwgmixer/world[@name='tiny']/property[@class='forest_countrytown']/property[@name='tiles']/@value">9, 10</set> <!-- 6, 6 -->	
	<set xpath="/rwgmixer/world[@name='tiny']/property[@class='bforest_countrytown']/property[@name='tiles']/@value">8, 9</set> <!-- 5, 6 -->
	<set xpath="/rwgmixer/world[@name='tiny']/property[@class='bforest_town']/property[@name='tiles']/@value">9, 10</set> <!-- 6, 7 -->
	<set xpath="/rwgmixer/world[@name='tiny']/property[@class='countrytown']/property[@name='tiles']/@value">8, 9</set> <!-- 5, 6 -->
	<set xpath="/rwgmixer/world[@name='tiny']/property[@class='town']/property[@name='tiles']/@value">10, 11</set> <!-- 7, 8 -->
	
	<!-- Small 5000 - 7000 RWG Fillers -->
	<set xpath="/rwgmixer/world[@name='small']/property[@class='oldwest']/property[@name='tiles']/@value">3, 4</set> <!-- 1, 2 -->
